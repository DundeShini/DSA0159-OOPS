#include<iostream>
using namespace std;
int main()
{
	int a,b,c;
	cout<<"enter the a value:";
	cin>>a;
	cout<<"enter the b value:";
	cin>>b;
	cout<<"enter the c value:";
	cin>>c;
	if(a<b && a<c)
	{
		cout<<"a is smallest number";
	}
	else if(b<a && b<c)
	{
		cout<<"b is smallest number";
	}
	else
	{
		cout<<"c is smallest number";
	}
 return 0;
}

output:
enter the a value:3
enter the b value:7
enter the c value:1
c is smallest number
