#include<iostream>
using namespace std;
int main()
{
	int a,b;
	cout<<"enter the value of a:";
	cin>>a;
	cout<<"enter the value of b:";
	cin>>b;
	a=a*b;
	b=a/b;
	a=a/b;
	cout<<"after swapping"<<endl<<"a="<<a<<endl<<"b="<<b;
	return 0;
}

output:
enter the value of a:5
enter the value of b:4
after swapping
a=4
b=5
